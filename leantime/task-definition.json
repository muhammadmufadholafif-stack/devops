{
  "family": "lks-leantime-app",
  "taskRoleArn": "arn:aws:iam::748381872886:role/LabRole",
  "executionRoleArn": "arn:aws:iam::748381872886:role/LabRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048",
  "volumes": [
    {
      "name": "lks-leantime-public-userfiles",
      "efsVolumeConfiguration": {
        "fileSystemId": "fs-09e8695dc43e5d460",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED",
        "authorizationConfig": {
          "accessPointId": "fsap-0ebcb99f57d286ffe",
          "iam": "ENABLED"
        }
      }
    },
    {
      "name": "lks-leantime-userfiles",
      "efsVolumeConfiguration": {
        "fileSystemId": "fs-09e8695dc43e5d460",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED",
        "authorizationConfig": {
          "accessPointId": "fsap-0cdf02f67239555fc",
          "iam": "ENABLED"
        }
      }
    },
    {
      "name": "lks-leantime-plugins",
      "efsVolumeConfiguration": {
        "fileSystemId": "fs-09e8695dc43e5d460",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED",
        "authorizationConfig": {
          "accessPointId": "fsap-05c0e3693c8ea97a7",
          "iam": "ENABLED"
        }
      }
    }
  ],


  "placementConstraints": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false,

  "containerDefinitions": [
    {
      "name": "leantime",
      "image": "leantime/leantime:latest",
      "cpu": 0,
      "essential": true,
      "volumesFrom": [],
      "systemControls": [],


      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "LEAN_DB_DATABASE",
          "valueFrom": "arn:aws:ssm:us-east-1:748381872886:parameter/lks/db/name"
        },
        {
          "name": "LEAN_DB_HOST",
          "valueFrom": "arn:aws:ssm:us-east-1:748381872886:parameter/lks/db/host"
        },
        {
          "name": "LEAN_DB_PASSWORD",
          "valueFrom": "arn:aws:ssm:us-east-1:748381872886:parameter/lks/db/password"
        },
        {
          "name": "LEAN_DB_PORT",
          "valueFrom": "arn:aws:ssm:us-east-1:748381872886:parameter/lks/db/port"
        },
        {
          "name": "LEAN_DB_USER",
          "valueFrom": "arn:aws:ssm:us-east-1:748381872886:parameter/lks/db/username"
        }
      ],
      "environment": [
        {
          "name": "PUID",
          "value": "1000"
        },
        {
          "name": "PGID",
          "value": "1000"
        },
        {
          "name": "LEAN_PORT",
          "value": "8080"
        },
        {
          "name": "LEAN_DEBUG",
          "value": "0"
        },
        {
          "name": "LEAN_SITENAME",
          "value": "LKS Leantime"
        },
        {
          "name": "LEAN_LANGUAGE",
          "value": "en-US"
        },
        {
          "name": "LEAN_DEFAULT_TIMEZONE",
          "value": "Asia/Jakarta"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "lks-leantime-public-userfiles",
          "containerPath": "/var/www/html/public/userfiles",
          "readOnly": false
        },
        {
          "sourceVolume": "lks-leantime-userfiles",
          "containerPath": "/var/www/html/userfiles",
          "readOnly": false
        },
        {
          "sourceVolume": "lks-leantime-plugins",
          "containerPath": "/var/www/html/app/Plugins",
          "readOnly": false
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lks/leantime-app",
          "awslogs-region": "us-east-1",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-stream-prefix": "ecs"
        }
      }

    }
  ]
}
